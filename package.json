  },
  "dependencies": {
    "classnames": "^2.3.2",
    "date-fns": "^2.30.0",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
